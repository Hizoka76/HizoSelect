#. TranslationAssistance ==============================> Texts of the errors
#: functions/translation.sh:28
msgid "The %s argument needs a value."
msgstr "L'argument %s nécessite une valeur."

#: functions/translation.sh:29
msgid "The number %s is out of selection."
msgstr "Le nombre %s est hors sélection."

#: functions/translation.sh:30
msgid "%s: %s is not an accepted value."
msgstr "%s : %s n'est pas une valeur acceptée."

#: functions/translation.sh:31
msgid "%s: %s is not in the proposals list."
msgstr "%s : %s n'est pas dans la liste des propositions."

#: functions/translation.sh:32
msgid "%s: The number of values is greater than the number of items."
msgstr "%s : Le nombre de valeur est plus élevé que le nombre de proposition."

#: functions/translation.sh:33
msgid "%s: Impossible to use %s because %s mode is disabled."
msgstr "%s : Impossible d'utiliser %s car le mode %s est désactivé."

#: functions/translation.sh:34
msgid "%s: Impossible to associate * with another value."
msgstr "%s : Impossible de coupler * avec une autre valeur."

#: functions/translation.sh:35
msgid ""
"The effects are define by a number between 0 and 255 for effects or by bold "
"dim smul smso."
msgstr ""
"Les effets sont définis par un nombre entre 0 et 255 pour les couleurs ou "
"par bold dim smul smso."

#: functions/translation.sh:36
msgid "%s: file not founded."
msgstr "%s : fichier introuvable."

#: functions/translation.sh:37
msgid "The input argument must have %%n."
msgstr "L'argument input nécessite obligatoirement l'utilisation de %%n."

#: functions/translation.sh:38
msgid "Insufficient number of items."
msgstr "Nombre de proposition insuffisant."

#: functions/translation.sh:39
msgid "%s key isn't usable."
msgstr "La touche %s n'est pas utilisable."

#: functions/translation.sh:40
msgid "Maximum limit reached."
msgstr "Limite maximale atteinte."

#: functions/translation.sh:41
msgid "Number of answer insufficient."
msgstr "Nombre de réponse insufisant."

#: functions/translation.sh:42
msgid "The --min value cannot be greater than the --max value."
msgstr "La valeur de --min ne peut être plus grande que celle de --max."

#: functions/translation.sh:43
msgid "%s: - needs a MIN value before and a MAX value after."
msgstr "%s : - nécessite une valeur MIN avant et une valeur MAX après."

#: functions/translation.sh:44
msgid "The second number must be greater than the first."
msgstr "Le 2ème nombre doit être plus grand que le 1er."

#: functions/translation.sh:45
msgid "The range needs a second number."
msgstr "La plage nécessite un 2ème nombre."

#: functions/translation.sh:46
msgid "The range will exceed the maximum answer number."
msgstr "La plage excédera la limite du nombre de réponse."

#: functions/translation.sh:47
msgid "A number must precede the - sign."
msgstr "Un nombre doit obligatoirement précéder le signe -."

#: functions/translation.sh:48
msgid "Impossible to add more - signs."
msgstr "Impossible de cumuler les signes -."

#: functions/translation.sh:49
msgid "This value range will unnecessarily duplicate."
msgstr "Cette nouvelle plage va faire doublon."

#: functions/translation.sh:50
msgid "The %s argument is unknow."
msgstr "L'argument %s est inconnu."

#: functions/translation.sh:51
msgid "%s number already entered."
msgstr "Nombre %s déjà inscrit."

#: functions/translation.sh:52
msgid "Please explain to hizo@free.fr how this error happened."
msgstr "Merci d'indiquer ce que vous avez fait à hizo@free.fr."

#: functions/translation.sh:53
msgid "%s: A number cannot start by 0."
msgstr "%s : Un nombre ne peut commencer par 0."

