msgid "item(s)     "
msgstr "proposition(s)"

#: functions/translation.sh:102
msgid "int   "
msgstr "entier"

#: functions/translation.sh:103
msgid "text "
msgstr "texte"

#: functions/translation.sh:104
msgid "Management of the content of the bar and its actions."
msgstr "Gestion du contenu de la barre et de ses actions."

#: functions/translation.sh:105
msgid "Origin of the proposals, command AND/OR stdin."
msgstr "Origine des propositions, commande ET/OU stdin."

#: functions/translation.sh:106
msgid "Delimiter to use between of the returned items."
msgstr "Délimiteur à utiliser entre les propositions retournées."

#: functions/translation.sh:107
msgid "Text to add in the help screen."
msgstr "Texte additionnel à afficher dans l'aide."

#: functions/translation.sh:108
msgid "HizoSelect command help"
msgstr "Aide de la commande HizoSelect"

#: functions/translation.sh:109
msgid "Name of the command displayed in error messages."
msgstr "Nom de la commande affichée dans les messages d'erreurs."

#: functions/translation.sh:110
msgid "Clear the terminal before display the texts."
msgstr "Nettoie le terminal avant d'afficher les textes."

#: functions/translation.sh:111
